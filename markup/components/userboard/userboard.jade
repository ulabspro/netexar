mixin customer-registration-form(data)
  .userboard
    .userboard-modal
      .userboard-modal__title Регистрация Заказчика
      .userboard-modal__subtitle С помощью аккаунта в соц. сетях
      .userboard-modal-social
        a(href="#" title="").userboard-modal-social__item._fb
        a(href="#" title="").userboard-modal-social__item._vk
        a(href="#" title="").userboard-modal-social__item._gp
        a(href="#" title="").userboard-modal-social__item._ok
      .userboard-modal__subtitle или

      form(method="" action="").form.customer-registration-form
        .form-group._iconed._name
          input(type="text" value="" placeholder="Фамилия").form-group__field
        .form-group._iconed._name
          input(type="text" value="" placeholder="Имя").form-group__field
        .form-group._iconed._name
          input(type="text" value="" placeholder="Отчество").form-group__field
        .form-group._iconed._phone
          input(type="text" value="" placeholder="Телефон").form-group__field
        .form-group._iconed._email
          input(type="text" value="" placeholder="E-mail").form-group__field
        .form-group._iconed._organization
          input(type="text" value="" placeholder="Наименование организации").form-group__field
        .form-group._iconed._password
          input(type="text" value="" placeholder="Пароль").form-group__field

        input(type="submit" value="Зарегистрироваться").form-reg-submit

        .form-privacy Нажимая на кнопку «Зарегистрироваться», вы подтверждаете свое согласие с условиями <a href="#" title="">Публичной офферты</a> и <a href="#" title="">Политикой конфиденциальности</a>

mixin performer-registration-form(data)
  .userboard
    .userboard-modal
      .userboard-modal__title Регистрация Исполнителя
      .userboard-modal__subtitle С помощью аккаунта в соц. сетях
      .userboard-modal-social
        a(href="#" title="").userboard-modal-social__item._fb
        a(href="#" title="").userboard-modal-social__item._vk
        a(href="#" title="").userboard-modal-social__item._gp
        a(href="#" title="").userboard-modal-social__item._ok
      .userboard-modal__subtitle или

      form(method="" action="").form.customer-registration-form
        .form-group._iconed._name
          input(type="text" value="" placeholder="Фамилия").form-group__field
        .form-group._iconed._name
          input(type="text" value="" placeholder="Имя").form-group__field
        .form-group._iconed._name
          input(type="text" value="" placeholder="Отчество").form-group__field
        .form-radio-group.jsChangeAcc
          label.form-radio.jsChangeAccItem
            input(type="radio" name="accType" value="Частник" checked="checked").form-radio__field
            span Частник
          label.form-radio.jsChangeAccItem
            input(type="radio" name="accType" value="АТП").form-radio__field
            span АТП
        .form-group._iconed._organization.jsChangeAccItemToggle
          input(type="text" value="" placeholder="Наименование организации").form-group__field
        .form-group._iconed._phone
          input(type="text" value="" placeholder="Телефон").form-group__field
